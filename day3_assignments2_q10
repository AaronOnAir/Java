package day3;

public class Question_10 {

	public static void main(String[] args) {

		int hour, minute, second;
		hour= 12;
		minute= 24;
		second= 33;
		
		String amOrPm1= "AM";
		String amOrPm2= "PM";
		
		System.out.println(hour + ":" + minute + ":" + second + " " + amOrPm1);
		System.out.println(hour + ":" + minute + ":" + second + " " + amOrPm2);
		
	}

}
